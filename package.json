{
  "name": "flappy-bird",
  "version": "1.0.0",
  "description": "flappy-bird game",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "webpack server --config webpack/dev.js",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/",
    "start": "npm run build && node dist/server/index.js",
    "build:client": "webpack --config webpack/prod.js",
    "build:server": "npm run clean && tsc --project ./src/server && tsc-alias && npm run copy-files",
    "start:server": "node dist/server/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:analyze": "webpack --config webpack/prod.js --env ANALYZE",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chosica-group/flappy-bird.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chosica-group/flappy-bird/issues"
  },
  "homepage": "https://github.com/chosica-group/flappy-bird#readme",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "^5.0.0-alpha.86",
    "@mui/material": "^5.8.1",
    "@reduxjs/toolkit": "^1.8.2",
    "express": "^4.18.1",
    "hot-module-reload": "^0.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  },
  "engines": {
    "node": "16.15.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/serviceworker": "^0.0.47",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.26.0",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-boundaries": "^2.10.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.6.2",
    "react-refresh": "^0.13.0",
    "react-refresh-typescript": "^2.0.5",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.3.0",
    "tsc-alias": "^1.6.11",
    "tsc-files": "^1.1.3",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "eslint --fix",
      "prettier --write",
      "tsc-files --noEmit"
    ]
  }
}
